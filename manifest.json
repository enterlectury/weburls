{
    "manifest_version": 3,
    "name": "weburls",
    "version": "1.0",
    "description": "Inserts a URL into the current tab's URL",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "assets/get.png",
      "48": "assets/get.png",
      "128": "assets/get.png"
    }
  }
